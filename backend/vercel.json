{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "http://dee-stories.vercel.app/(.*)",
        "headers": {
          "Location": "https://dee-stories.vercel.app/$1"
        },
        "status": 308
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; includeSubDomains; preload"
          }
        ]
      }
    ]
  }
